{
  "title": "KlantContactMoment to BetrokkeneBijKlantContact POST",
  "description": "Makes sure there are not multiple variants of the same categorie.",
  "$id": "https://commongateway.nl/mapping/klant.klantContactMomentToBetrokkeneBijKlantContactPOST.mapping.json",
  "$schema": "https://docs.commongateway.nl/schemas/Mapping.schema.json",
  "version": "0.0.1",
  "passTrough": false,
  "mapping": {
		"klantcontact": "{% if contactmoment|default %}{{ contactmoment }}{% endif %}",	
		"partij": "{% if klant|default %}{{ klant }}{% endif %}"
  },
	"cast": {
		"klantcontact": "unsetIfValue==",
		"partij": "unsetIfValue=="
	}
}
