{
    "title": "BetrokkeneBijKlantContact to KlantContactMoment item",
    "description": "Makes sure there are not multiple variants of the same categorie.",
    "$id": "https://commongateway.nl/mapping/klant.betrokkeneBijKlantContactToKlantContactMomentItem.mapping.json",
    "$schema": "https://docs.commongateway.nl/schemas/Mapping.schema.json",
    "version": "0.0.19",
    "passTrough": false,
    "mapping": {
        "contactmoment": "{% if klantcontact|default('null') %}{{ map('https://commongateway.nl/mapping/klant.klantContactToContactmoment.mapping.json', klantcontact)|json_encode }}{% endif %}",	
        "klant": "{% if partij|default('null') %}{{ map('https://commongateway.nl/mapping/klant.partijToKlantItem.mapping.json', partij)|json_encode }}{% endif %}"
    },
    "cast": {
        "contactmoment": "jsonToArray",
        "klant": "jsonToArray"
    }
}
  